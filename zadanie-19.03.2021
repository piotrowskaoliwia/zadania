list_ = [56, 10, 89, 8, 34, 67, 24]
print(list_)
print()

#srednia arytmetyczna

for el in list_ :
    dlugosc_listy = 0
    d = 0
for el in list_ :
    dlugosc_listy += 1
    d += el
else:
    srednia1 = (d/dlugosc_listy)
    print('srednia arytmetyczna:')
    print(srednia1)
    print()


#srednia harmoniczna

for el in list_ :
    dlugosc_listy = 0
    h = 0
for el in list_ :
    dlugosc_listy += 1
    h += 1/el
else:
    srednia2 = (h/dlugosc_listy)
    print('srednia harmoniczna:')
    print(srednia2)
    print()
    
